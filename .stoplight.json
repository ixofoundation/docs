{
  "exclude": [
        ".gitbook/*.md",
        ".github/*.md",
        "concepts/*.md",
        "developer-tools/*.md",
        "developers/*.md",
        "guides/*.md",
        "ixo/*.md",
        "models/*.md",
        "policies/*.md",
        "portals/*.md",
        "purpose/*.md",
        "research/*.md",
        "resources/*.md",
        "services/*.md",
        "start/*.md",
        "README.md",
        "SUMMARY.md"
  ],
  "formats": {
    "openapi": {
      "rootDir": "reference",
      "include": [
        "**"
      ]
    },
    "json_schema": {
      "rootDir": "models",
      "include": [
        "**"
      ]
    },
    "markdown": {
      "rootDir": ".",
      "include": [
        "**/*.md"
      ]
    },
    "image": {
      "rootDir": "assets/images"
    }
  }
}